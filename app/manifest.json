{
  "manifest_version": 2,

  "name": "FB Ad Slots",
  "description": "This extension collects the insertion points of ads in a Facebook feed.",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "chrome://favicon/",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "icons": {
          "128": "icon_128.png" },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["content_script.js", "/modules/parseURI.js", "/modules/get_FbAdLinks.js"],
    "run_at": "document_idle",
    "all_frames": true
  }]
}
